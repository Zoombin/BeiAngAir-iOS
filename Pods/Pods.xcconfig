GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers" "${PODS_ROOT}/Headers/ASIHTTPRequest" "${PODS_ROOT}/Headers/JSONKit" "${PODS_ROOT}/Headers/MBProgressHUD" "${PODS_ROOT}/Headers/MMDrawerController" "${PODS_ROOT}/Headers/MMProgressHUD" "${PODS_ROOT}/Headers/Reachability" "${PODS_ROOT}/Headers/Toast" "${PODS_ROOT}/Headers/ZBUtilities" "${PODS_ROOT}/Headers/iCarousel" "$(SDKROOT)/usr/include/libxml2"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers" -isystem "${PODS_ROOT}/Headers/ASIHTTPRequest" -isystem "${PODS_ROOT}/Headers/JSONKit" -isystem "${PODS_ROOT}/Headers/MBProgressHUD" -isystem "${PODS_ROOT}/Headers/MMDrawerController" -isystem "${PODS_ROOT}/Headers/MMProgressHUD" -isystem "${PODS_ROOT}/Headers/Reachability" -isystem "${PODS_ROOT}/Headers/Toast" -isystem "${PODS_ROOT}/Headers/ZBUtilities" -isystem "${PODS_ROOT}/Headers/iCarousel"
OTHER_LDFLAGS = -ObjC -lxml2.2 -lz.1 -framework CFNetwork -framework CoreGraphics -framework MobileCoreServices -framework QuartzCore -framework SystemConfiguration
PODS_ROOT = ${SRCROOT}/Pods